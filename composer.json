{
    "name": "cortex/statistics",
    "description": "Cortex Statistics is a frontend layer for lightweight, yet detailed package for tracking and recording user visits across your Laravel application. With only one simple query per request, important data is being stored, and later a cronjob crush numbers to extract meaningful stories from within the haystack.",
    "type": "cortex-module",
    "keywords": [
        "user",
        "model",
        "rinvex",
        "laravel",
        "eloquent",
        "statistics",
        "tracking",
        "visitor",
        "agents",
        "client",
        "views",
        "device",
        "logging",
        "platform",
        "browser"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-statistics/issues",
        "source": "https://github.com/rinvex/cortex-statistics",
        "docs": "https://github.com/rinvex/cortex-statistics/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-statistics/contributors"
        }
    ],
    "require": {
        "php": "^7.2.0",
        "Illuminate/auth": "~5.8.0",
        "Illuminate/console": "~5.8.0",
        "Illuminate/http": "~5.8.0",
        "Illuminate/routing": "~5.8.0",
        "Illuminate/support": "~5.8.0",
        "cortex/foundation": "^2.0.0",
        "davejamesmiller/laravel-breadcrumbs": "^5.2.0",
        "laravelcollective/html": "~5.8.0",
        "league/fractal": "^0.17.0",
        "proengsoft/laravel-jsvalidation": "^2.4.0",
        "rinvex/laravel-composer": "^2.0.0",
        "rinvex/laravel-statistics": "^2.0.0",
        "rinvex/laravel-menus": "^2.0.0",
        "rinvex/laravel-support": "^2.0.0",
        "rinvex/laravel-tenants": "^2.0.0",
        "silber/bouncer": "v1.0.0-rc.5",
        "spatie/laravel-activitylog": "^3.6.0",
        "yajra/laravel-datatables-buttons": "^4.6.0",
        "yajra/laravel-datatables-fractal": "^1.4.0",
        "yajra/laravel-datatables-html": "^4.4.0",
        "yajra/laravel-datatables-oracle": "^9.3.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.26.0",
        "phpunit/phpunit": "^8.1.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "Cortex\\Statistics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Statistics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Statistics\\Providers\\StatisticsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
